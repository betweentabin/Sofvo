{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://angelic-rebirth-production-769f.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "framework": "vite"
}
