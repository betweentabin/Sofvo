{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "https://angelic-rebirth-production-769f.up.railway.app/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "framework": "vite"
}
